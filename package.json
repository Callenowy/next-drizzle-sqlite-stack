{
  "name": "next-drizzle-sqlite-stack",
  "version": "0.1.0",
  "private": true,
  "repository": "git@github.com:Callenowy/next-drizzle-sqlite-stack",
  "license": "MIT",
  "author": "Callenowy",
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,md}\"",
    "format:check": "prettier --list-different \"**/*.{ts,tsx,js,jsx,md}\"",
    "test": "npm run test:unit",
    "test:unit": "vitest",
    "test:unit:coverage": "vitest run --coverage",
    "test:unit:ui": "vitest --ui",
    "postinstall": "husky install",
    "db:migration:generate": "drizzle-kit generate:sqlite",
    "db:migration:run": "tsx ./src/db/migrate.ts",
    "db:check": "drizzle-kit up:sqlite",
    "db:studio": "drizzzle-kit studio",
    "db:seed": "tsx ./src/db/seed.ts"
  },
  "dependencies": {
    "@paralleldrive/cuid2": "^2.2.2",
    "better-sqlite3": "^9.3.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "dotenv": "^16.3.1",
    "drizzle-orm": "^0.29.3",
    "i18n-iso-countries": "^7.7.0",
    "next": "14.0.4",
    "react": "^18",
    "react-dom": "^18",
    "tailwind-merge": "^2.2.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.4",
    "@commitlint/config-conventional": "^18.4.4",
    "@faker-js/faker": "^8.3.1",
    "@testing-library/jest-dom": "^6.2.0",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.2",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/better-sqlite3": "^7.6.8",
    "@types/node": "^20",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.2.1",
    "@vitest/ui": "^1.2.1",
    "autoprefixer": "^10.0.1",
    "drizzle-kit": "^0.20.13",
    "eslint": "^8",
    "eslint-config-next": "14.0.4",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-jest-dom": "^5.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "eslint-plugin-testing-library": "^6.2.0",
    "husky": "^8.0.3",
    "jsdom": "^23.2.0",
    "lint-staged": "^15.2.0",
    "postcss": "^8",
    "prettier": "^3.2.4",
    "prettier-plugin-tailwindcss": "^0.5.11",
    "tailwindcss": "^3.3.0",
    "tsx": "^4.7.0",
    "typescript": "^5",
    "vite-tsconfig-paths": "^4.3.1",
    "vitest": "^1.2.1"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not dead"
  ],
  "engines": {
    "node": ">=20.8.0",
    "npm": ">=10.2.4"
  }
}
